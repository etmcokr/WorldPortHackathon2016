{
  "ecmaVersion": 6,
  "libs": [],
  "loadEagerly": [
    "**/*.js"
  ],
  "dontLoad": [
    "**/node_modules/**"
  ],
  "plugins": {
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "es_modules": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": false
    }
  }
}